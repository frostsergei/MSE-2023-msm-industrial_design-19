<UserControl x:Class="Setup_database_for_device.View.SystemForm.PipelineControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Setup_database_for_device.View.SystemForm"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="800" BorderBrush="#eee" BorderThickness="1">
    <UserControl.Resources>
        <Style x:Key="Enabled">

        </Style>
    </UserControl.Resources>
    <Grid Name="PipelineControlBlock">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.7*"></ColumnDefinition>
            <ColumnDefinition Width="0.3*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Name="PipelineSettingLabel" Grid.Row="0" Grid.Column="0"/>
    </Grid>
</UserControl>
